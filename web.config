<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <system.webServer>
    <rewrite>
      <rules>
        <rule name="note">
          <match url="^([a-zA-Z0-9_-]+)$" />
          <action type="Rewrite" url="index.php?note={R:1}" />
        </rule>
      </rules>
    </rewrite>
  </system.webServer>
</configuration>
